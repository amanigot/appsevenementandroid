<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    
	<!-- etat selectionne -->
	<item android:state_selected="true" 
	    android:state_focused="false"
		android:state_pressed="false" 
		android:drawable="@drawable/btnmesactusselect" />
	
	<!--  Inactive tab : etat classique-->
	<item android:state_selected="false" 
	    android:state_focused="false"
		android:state_pressed="false"
		 android:drawable="@drawable/btnmesactus" />
	
	<!--  Pressed tab  etat presse non selectionne-->
	<item android:state_pressed="true" 
	    android:state_selected="false"
	    android:drawable="@drawable/btnmesactuspressed" />
	
	<!-- etat presse selectionne -->
	<item android:state_pressed="true" 
	    android:state_selected="true"
	    android:drawable="@drawable/btnmesactuspressedselect" />
	
	
</selector>
